pragma solidity ^0.8.13;

import "forge-std/Script.sol";
import {Authorization} from "../src/Authorization.sol";
import {Auction} from "../src/Auction.sol";
import {AuctionNFT} from "../src/AuctionNFT.sol";
import "../src/interface/IAutomationRegistrarInterface.sol";
import "../src/interface/IAutomationRegistryInterface.sol";
import {LinkTokenInterface} from "../lib/chainlink/contracts/src/v0.8/shared/interfaces/LinkTokenInterface.sol";


contract Deploy is Script {
    Authorization public dAuthorization;
    Auction public dAuction;
    AuctionNFT public dAuctionNFT;

    function setUp() public {}

    function run() public {
        uint privateKey = vm.envUint("PRIVATE_KEY");
        vm.startBroadcast(privateKey);

        dAuthorization = new Authorization();

        address _MINTINGADDRESS = address(0);
        string memory _WINNERIMAGEURI = "data:image/jpg;base64,";
        
        string memory _PARTICIPANTIMAGEURI = "data:image/jpg;base64,";
        
        dAuctionNFT = new AuctionNFT(_MINTINGADDRESS);
        dAuctionNFT.updateWinnerImageURI(_WINNERIMAGEURI);
        dAuctionNFT.updateParticpantImageURI(_PARTICIPANTIMAGEURI);
        
        address _TEAMWALLET = 0xc85a04f7ea44f2690a7e5dD4C525DD5C81412d98;
        LinkTokenInterface LINK = LinkTokenInterface(0x0b9d5D9136855f6FEc3c0993feE6E9CE8a297846);
        IAutomationRegistrarInterface REGISTRAR = IAutomationRegistrarInterface(0xD23D3D1b81711D75E1012211f1b65Cc7dBB474e2);
        IAutomationRegistryInterface REGISTRY = IAutomationRegistryInterface(0x819B58A646CDd8289275A87653a2aA4902b14fe6);
        string memory _ADMINEMAIL = "okeolamobolaji19@gmail.com";
        uint32 _GASLIMIT = 999999;
        uint96 _AMOUNT = 20000000000000000000;
        address _ADMINADDRESS = 0xF8489730751cE4a62ad99ab88F15211B9F027C9e;

        dAuction = new Auction(address(dAuthorization), address(dAuctionNFT), _TEAMWALLET, LINK, REGISTRAR, REGISTRY, _ADMINEMAIL, _GASLIMIT, _AMOUNT, _ADMINADDRESS);


        dAuctionNFT.updateMintingAddress(address(dAuction));

        vm.stopBroadcast();
    }
}